{
    "name": "xiaosumay/snake",
    "description": "thinkphp5做的通用系统改后台",
    "type": "project",
    "keywords": [
        "snake",
        "thinkphp"
    ],
    "homepage": "https://github.com/xiaosumay/snake",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "xiaosumay",
            "email": "xiaosumay@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "topthink/framework": "5.1.32",
        "topthink/think-captcha": "2.*",
        "topthink/think-image": "^1.0",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "topthink/think-migration": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "application"
        }
    },
    "extra": {
        "think-path": "thinkphp"
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
