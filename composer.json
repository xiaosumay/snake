{
    "name": "cyclone/snake",
    "description": "thinkphp5做的通用系统改后台",
    "type": "project",
    "keywords": [
        "snake",
        "thinkphp"
    ],

    "homepage": "https://github.com/xiaosumay/snake",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "xiaosumay",
            "email": "xiaosumay@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "topthink/framework": "5.1.*",
        "topthink/think-captcha": "2.*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*"
